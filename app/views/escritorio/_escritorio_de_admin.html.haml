.row-fluid
	.span3
		.well.sidebar-nav
			%h1= @title
			%h5 Acciones
			%ul.nav.nav-pills.nav-stacked
				%li
					= link_to "Crear Usuario", crearusuario_path
				%li
					= link_to "Trabajar con Usuarios", usuarios_path
				%li
					= link_to "Trabajar con Turnos", turnos_path
				%li
					= link_to "Ver Actividad", actividades_path
	.span9
		.row-fluid
			.span5
				%h2 Ultimos usuarios
				= render 'ultimos_usuarios', :usuarios => @ultimosUsuarios
			.span7
				%h2 Visor de actividad reciente
				= render '/actividades/log_actividad', :actividades => @actividades
=render 'layouts/footer'
